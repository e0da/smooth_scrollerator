{
  "name": "Smooth Scrollerator",
  "version": "1.1.2",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "web_accessible_resources": [
    "img/cursor.png"
  ],
  "background": {
    "scripts": [
      "vendor/ga.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [ "src/sscr.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
    }
  ],
  "description": "Scroll smoothly",
  "icons": {
    "128": "img/128n.png",
    "16": "img/16n.png",
    "32": "img/32n.png",
    "48": "img/48n.png"
  }
}
