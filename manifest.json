{
  "name": "Smooth Scrollerator",
  "version": "1.1.1",
  "manifest_version": 2,
  "web_accessible_resources": [
    "img/cursor.png"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "js": [ "src/sscr.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
    }
  ],
  "description": "Scroll smoothly",
  "icons": {
    "128": "img/128n.png",
    "16": "img/16n.png",
    "32": "img/32n.png",
    "48": "img/48n.png"
  }
}
